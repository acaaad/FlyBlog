<!DOCTYPE HTML>
<html>

<head>
  <title>ARArt</title>
  <!--<meta name="description" content="website description" />
  <meta name="keywords" content="website keywords, website keywords" />
  <meta http-equiv="content-type" content="text/html; charset=UTF-8" />-->
  <link href="/../css/style.css" rel="stylesheet" type="text/css" />
  <link href="/../css/dark.css" rel="stylesheet" type="text/css" />
  <script type="text/javascript" src="js/modernizr-1.5.min.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.14/angular.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.14/angular-route.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.14/angular-resource.js"></script>
  <script src="/../js/test.js"></script>
</head>

<body ng-app="app" ng-controller="MainCtrl">
  <div id="main">

    <!-- begin header -->
    <header>
      <div id="logo"><h1>GROUP<a href="#">#1</a></h1></div>
      <nav>
        <ul class="sf-menu" id="nav">
          <li><a href="about.html">about me</a></li>
          <li class="selected"><a href="blog.html">blog</a></li>
          <li><a href="../logout">logout</a></li>
        </ul>
      </nav>
    </header>
    <!-- end header -->

    <!-- begin content -->
    <div id="site_content">
      <div id="left_content">
        <p><button id="createnew" name="createnew" style="padding: 2pt" ng-click="addnew()">Create New Post</button></p>
        <div id="blog_container" >
          <h4><strong> Title: </strong><%= post[0]["title"] %> <br/></h4>
          <h4><strong> Post: </strong><%= post[0]["post"] %><br/></h4>
          <!--<strong> Post: </strong>{{post.post}}<br><br>-->
          <script type='text/javascript'>
            // rows = <%-JSON.parse(JSON.stringify(post))%>
            var jeson = '{"0":{"id":"35","name":"Ahamed shajeer","date":"2014-03-03","age":"25"},"1":{"id":"36","name":"Meshajeer","date":"0000-00-00","age":"25"},"2":{"id":"37","name":"Iam shajeer","date":"0000-00-00","age":"25"},"average_age":25,"count":3}';
            obj = JSON.parse(jeson);

            document.write();
          </script>

          <!--<div class="blog"><h2>{{bulan}}</h2><h3>{{tanggal}}</h3></div>
          <h4 class="select"><a onclick="showpost()">{{pos}}</a></h4>
          <p>{{contentpreview}} <a onclick="showpost()">read more.....</a></p>-->

        <h1></h1>
        </div>
      </div>
      <div id="right_content">
        <div id="blog_text">
          <h1>{{blogtitle}}</h1>
          <p>{{blogcontent}}</p>
          <ul class="sf-menu" id="editpost">
            <li style="padding-right: 12pt"><a href="edit.html">Edit </a></li>
            <li><a href="home.html">Delete</a></li>
          </ul>
          <form style="padding-top: 20pt" ng-submit="submitcomment()">
            <br>Add Comment:<br/><br/>
            <textarea id="addcomment" cols="55" rows="5"></textarea><br/><br/>
            <input type="submit" value="Submit" style="padding: 3pt"><br/><br/>
          </form>
          <hr>
          <div class="commentsection" id="commentsection">
            <p>
              <b>{{commentID}}:</b><br/>
              {{comment}}<br/></p>


            <hr>
          </div>
        </div>
      </div>
    </div>
    <!-- end content -->

    <!-- begin footer -->
    <footer>
      <p>Copyright &copy; 2016 Group1</p>
    </footer>
    <!-- end footer -->

  </div>
  <!-- javascript at the bottom for fast page loading -->
  <script type="text/javascript" src="js/jquery.min.js"></script>
  <script type="text/javascript" src="js/jquery.easing-sooper.js"></script>
  <script type="text/javascript" src="js/jquery.sooperfish.js"></script>
  <!-- initialise sooperfish menu -->
  <script type="text/javascript">
    $(document).ready(function() {
      $('ul.sf-menu').sooperfish();
    });
  </script>
</body>
</html>
